{
	"name": "Memory usage is high on {{host.name}} - {{host.ip}}",
	"type": "query alert",
	"query": "avg(last_5m):( avg:system.mem.used{*} by {host} / avg:system.mem.total{*} by {host} ) * 100 > 90",
	"message": "Memory usage is high on {{host.name}} - {{host.ip}}",
	"tags": [
		"system",
		"default"
	],
	"options": {
		"notify_audit": false,
		"locked": false,
		"timeout_h": 0,
		"silenced": {},
		"include_tags": false,
		"no_data_timeframe": null,
		"require_full_window": true,
		"new_host_delay": 300,
		"notify_no_data": false,
		"renotify_interval": 0,
		"escalation_message": "",
		"thresholds": {
			"critical": 90,
			"warning": 80
		}
	}
}