{
	"name": "AWS Service Limit",
	"type": "metric alert",
	"query": "avg(last_5m):avg:aws.advisor.service_limit.usage_ratio{*} by {account-id,limit_name} > 0.9",
	"message": "",
	"tags": [],
	"options": {
		"notify_audit": false,
		"locked": false,
		"timeout_h": 0,
		"silenced": {},
		"include_tags": false,
		"no_data_timeframe": null,
		"require_full_window": true,
		"new_host_delay": 300,
		"notify_no_data": false,
		"renotify_interval": 0,
		"escalation_message": "",
		"thresholds": {
			"critical": 0.9,
			"warning": 0.7
		}
	}
}